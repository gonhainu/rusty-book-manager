  "query": "\n                SELECT\n                    c.checkout_id,\n                    c.book_id,\n                    u.user_id,\n                    u.name AS user_name,\n                    c.checked_out_at AS checked_out_at\n                FROM checkouts AS c\n                INNER JOIN users AS u USING(user_id)\n                WHERE c.book_id = ANY($1)\n                ;\n            ",
